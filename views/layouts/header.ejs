<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Training Module</title>
     <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css"/>
   <script src="https://code.jquery.com/jquery-2.2.0.min.js"></script>
</head>
<body>
  <div class="container">
    <!-- Static navbar -->
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <% if( req.session.user[0].user_role == "SME"){ %>
              <li class="active"><a href="/questions/add">New Training</a></li>
            <% } else if (req.session.user[0].user_role == "Change Manager") { %>
              <li class="active"><a href="/questions/add">New Training</a></li>
              <li><a href="/questions">Training Phrases</a></li>
              <li><a href="/context_questions">Context Queries</a></li>
            <% } %>
          </ul>
          <ul class="nav navbar-nav navbar-right">
          	<% if(req.session.user){ %>
              <li><a href="#">Login as <%= req.session.user[0].user_role %></a></li>  
							<li><a href="/users/logout">Logout</a></li>		
						<% } else{ %> 
							<li><a href="/users/signin">SignIn</a></li>
							<li><a href="/users/signup">SignUp</a></li> 
						<% } %>
          </ul>
        </div><!--/.nav-collapse -->
      </div><!--/.container-fluid -->
    </nav>

    <div class="container-fluid" style="margin-left: 0px;margin-right: 0px; padding-left: 0px; padding-right: 0px;">
 